{
  "name": "volateq-web-app",
  "version": "1.5.6",
  "description": "VueJs Web Application powered by Volateq",
  "main": "main.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "ENV=production vue-cli-service build",
    "make-version": "version=$(grep '\"version\":' package.json | sed 's/.*\"version\": \"\\([^\",]*\\)\",/\\1/') && sed -i 's/\"\\/js\\/app\\.js\"/\"\\/js\\/app\\.js?v='\"$version\"'\"/g' dist/index.html",
    "lint": "vue-cli-service lint",
    "vuetype": "vuetype"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VolaTeQ/volateq-web-app.git"
  },
  "author": "Jeremy Gauchel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VolaTeQ/volateq-web-app/issues"
  },
  "homepage": "https://github.com/VolaTeQ/volateq-web-app#readme",
  "dependencies": {
    "axios": "0.19.2",
    "bootstrap": "4.5.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "3.6.5",
    "direct-vuex": "0.12.0",
    "ol": "^6.6.1",
    "resumablejs": "1.1.0",
    "vue": "2.6.12",
    "vue-class-component": "7.2.6",
    "vue-i18n": "8.20.0",
    "vue-property-decorator": "9.0.0",
    "vue-router": "3.3.4",
    "vuex": "3.5.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-typescript": "4.4.6",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/babel-preset-app": "4.5.14",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "prettier": "^1.19.1",
    "sass": "^1.35.2",
    "sass-loader": "9.0.2",
    "typescript": "~4.1.5",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaFeatures": {
        "legacyDecorators": true
      }
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
